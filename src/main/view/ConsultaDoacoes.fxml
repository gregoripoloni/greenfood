    <children>
        <Label layoutX="200" layoutY="20" text="Doações Cadastradas">
            <font><Font size="20.0" /></font>
        </Label>
        <ScrollPane layoutX="99.0" layoutY="62.0" prefHeight="277.0" prefWidth="402.0">
            <content>
                <VBox fx:id="listaDoacoes" prefHeight="348.0" prefWidth="404.0" />
            </content>
        </ScrollPane>
        <Button fx:id="botaoFechar" layoutX="250" layoutY="360" onAction="#fecharJanela" prefWidth="100" text="Fechar" />
    </children>
</AnchorPane>
